<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CustomerController">
    <children>
        <TableView fx:id="CustomerTable" layoutX="28.0" layoutY="50.0" prefHeight="320.0" prefWidth="532.0">
            <columns>
                <TableColumn fx:id="CustomerIdCol" prefWidth="27.0" text="ID" />
                <TableColumn fx:id="CustomerNameCol" prefWidth="99.0" text="Name" />
                <TableColumn fx:id="CustomerAddressCol" prefWidth="89.0" text="Address" />
                <TableColumn fx:id="CustomerPostalCol" prefWidth="73.0" text="Postal" />
                <TableColumn fx:id="CustomerPhoneCol" prefWidth="135.0" text="Phone Number" />
                <TableColumn fx:id="CustomerStateCol" prefWidth="108.0" text="State" />
            </columns>
        </TableView>
        <Label layoutX="28.0" layoutY="14.0" prefHeight="31.0" prefWidth="126.0" text="Customer List" textAlignment="CENTER">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Button fx:id="EditCustomer" layoutX="575.0" layoutY="125.0" mnemonicParsing="false" onAction="#OnEditCustomer" prefHeight="50.0" prefWidth="215.0" text="Edit Customer">
            <font>
                <Font size="20.0" />
            </font></Button>
        <Button fx:id="AddCustomer" layoutX="575.0" layoutY="50.0" mnemonicParsing="false" onAction="#OnAddCustomer" prefHeight="50.0" prefWidth="215.0" text="Add Customer">
            <font>
                <Font size="20.0" />
            </font></Button>
        <Button fx:id="DeleteCustomer" layoutX="575.0" layoutY="200.0" mnemonicParsing="false" onAction="#OnDeleteCustomer" prefHeight="50.0" prefWidth="215.0" text="Delete Customer">
            <font>
                <Font size="20.0" />
            </font></Button>
        <Button fx:id="CustomerBackBtn" layoutX="575.0" layoutY="345.0" mnemonicParsing="false" onAction="#OnCustomerBackBtn" prefHeight="25.0" prefWidth="50.0" text="Back" />
    </children>
</AnchorPane>
