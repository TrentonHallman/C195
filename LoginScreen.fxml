<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
    <children>
        <Label fx:id="LoginLabel" alignment="CENTER" layoutX="100.0" layoutY="40.0" prefHeight="50.0" prefWidth="200.0" text="Login" textAlignment="CENTER">
            <font>
                <Font size="40.0" />
            </font>
        </Label>
        <TextField fx:id="LogInUsername" layoutX="75.0" layoutY="120.0" prefHeight="30.0" prefWidth="250.0" promptText="Username" />
        <TextField fx:id="LogInPassword" layoutX="75.0" layoutY="178.0" prefHeight="30.0" prefWidth="250.0" promptText="Password" />
        <Button fx:id="LogInSubmit" layoutX="76.0" layoutY="227.0" mnemonicParsing="false" onAction="#OnLogInSubmit" prefHeight="31.0" prefWidth="250.0" text="Login">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Label fx:id="Location" layoutX="17.0" layoutY="344.0" text="Location: ">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Label fx:id="LocationData" layoutX="17.0" layoutY="369.0" text="United States of America" />
    </children>
</AnchorPane>
