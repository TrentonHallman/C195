<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.EditCustomerController">
    <children>
        <Label layoutX="25.0" layoutY="14.0" text="Edit Customer">
            <font>
                <Font size="30.0" />
            </font>
        </Label>
        <Label layoutX="30.0" layoutY="80.0" text="ID:">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Label layoutX="30.0" layoutY="120.0" text="Name:">
            <font>
                <Font size="16.0" />
            </font></Label>
        <Label layoutX="30.0" layoutY="160.0" text="Address:">
            <font>
                <Font size="16.0" />
            </font></Label>
        <Label layoutX="30.0" layoutY="200.0" text="Postal Code:">
            <font>
                <Font size="16.0" />
            </font></Label>
        <Label layoutX="30.0" layoutY="280.0" text="Country:">
            <font>
                <Font size="16.0" />
            </font></Label>
        <Label layoutX="30.0" layoutY="240.0" text="Phone Number:">
            <font>
                <Font size="16.0" />
            </font></Label>
        <Label layoutX="30.0" layoutY="320.0" text="State/Province:">
            <font>
                <Font size="16.0" />
            </font></Label>
        <ComboBox fx:id="EditCountryCombo" layoutX="176.0" layoutY="280.0" onAction="#OnEditCountryCombo" prefWidth="150.0" />
        <ComboBox fx:id="EditDivisionCombo" layoutX="177.0" layoutY="320.0" prefWidth="150.0" />
        <TextField fx:id="EditPhone" layoutX="177.0" layoutY="240.0" />
        <TextField fx:id="EditPostal" layoutX="176.0" layoutY="200.0" />
        <TextField fx:id="EditAddress" layoutX="176.0" layoutY="160.0" />
        <TextField fx:id="EditName" layoutX="176.0" layoutY="120.0" />
        <TextField fx:id="EditID" alignment="CENTER" editable="false" layoutX="176.0" layoutY="80.0" prefHeight="25.0" prefWidth="40.0" promptText="X" />
        <Button fx:id="EditSaveBtn" layoutX="330.0" layoutY="360.0" mnemonicParsing="false" onAction="#OnEditSaveBtn" prefWidth="50.0" text="Save" />
        <Button fx:id="EditBackBtn" layoutX="25.0" layoutY="360.0" mnemonicParsing="false" onAction="#OnEditBackBtn" prefHeight="25.0" prefWidth="50.0" text="Back" />
        <Button fx:id="EditClearBtn" layoutX="270.0" layoutY="360.0" mnemonicParsing="false" onAction="#OnEditClearBtn" prefHeight="25.0" prefWidth="50.0" text="Clear" />
    </children>
</AnchorPane>
