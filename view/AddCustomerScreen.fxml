<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddCustomerController">
    <children>
        <Label layoutX="25.0" layoutY="14.0" text="Add Customer">
            <font>
                <Font size="30.0" />
            </font>
        </Label>
        <Label layoutX="30.0" layoutY="80.0" text="ID:">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Label layoutX="30.0" layoutY="120.0" text="Name:">
            <font>
                <Font size="16.0" />
            </font></Label>
        <Label layoutX="30.0" layoutY="160.0" text="Address:">
            <font>
                <Font size="16.0" />
            </font></Label>
        <Label layoutX="30.0" layoutY="200.0" text="Postal Code:">
            <font>
                <Font size="16.0" />
            </font></Label>
        <Label layoutX="30.0" layoutY="280.0" text="Country:">
            <font>
                <Font size="16.0" />
            </font></Label>
        <Label layoutX="30.0" layoutY="240.0" text="Phone Number:">
            <font>
                <Font size="16.0" />
            </font></Label>
        <Label layoutX="30.0" layoutY="320.0" text="State/Province:">
            <font>
                <Font size="16.0" />
            </font></Label>
        <ComboBox fx:id="AddCountryCombo" layoutX="176.0" layoutY="280.0" onAction="#OnAddCountryCombo" prefWidth="150.0" />
        <ComboBox fx:id="AddDivisionCombo" layoutX="177.0" layoutY="320.0" prefWidth="150.0" />
        <TextField fx:id="AddPhone" layoutX="177.0" layoutY="240.0" />
        <TextField fx:id="AddPostal" layoutX="176.0" layoutY="200.0" />
        <TextField fx:id="AddAddress" layoutX="176.0" layoutY="160.0" />
        <TextField fx:id="AddName" layoutX="176.0" layoutY="120.0" />
        <TextField fx:id="AddID" alignment="CENTER" editable="false" layoutX="176.0" layoutY="80.0" prefHeight="25.0" prefWidth="40.0" promptText="X" />
        <Button fx:id="AddSaveBtn" layoutX="330.0" layoutY="360.0" mnemonicParsing="false" onAction="#OnAddSaveBtn" prefWidth="50.0" text="Save" />
        <Button fx:id="AddBackBtn" layoutX="25.0" layoutY="360.0" mnemonicParsing="false" onAction="#OnAddBackBtn" prefHeight="25.0" prefWidth="50.0" text="Back" />
        <Button fx:id="AddClearBtn" layoutX="270.0" layoutY="360.0" mnemonicParsing="false" onAction="#OnAddClearBtn" prefHeight="25.0" prefWidth="50.0" text="Clear" />
    </children>
</AnchorPane>
